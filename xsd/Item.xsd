<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:it="http://www.univ-grenoble-alpes.fr/Item"
           targetNamespace="http://www.univ-grenoble-alpes.fr/Item"
           elementFormDefault="qualified">

    <xs:element name="Item" type="it:Item"/>

    <!-- Type ComplÃ¨xe -->
    <xs:complexType name="Item">
        <xs:sequence>
            <xs:element name="Id" type="it:Id"/>
        </xs:sequence>
    </xs:complexType>

    <!-- Type Simple -->

    <xs:simpleType name="Id">
        <xs:restriction base="xs:int">
            <xs:pattern value="[0-9]{2}"/>
        </xs:restriction>
    </xs:simpleType>
</xs:schema>